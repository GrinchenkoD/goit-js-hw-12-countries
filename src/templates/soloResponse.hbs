{{#each this}}
<div class="card-wrapper">
    <h2 class="title">{{name}}</h2>
    <div class="country-info">
        <div>
            <p class="card-item"><span>Capital:</span>{{capital}}</p>
            <p class="card-item"><span>Population:</span>{{population}}</p>
            <ul class="list">Languages:
                {{#each languages}}
                <li class="list-item">{{this.name}}</li>
                {{/each}}
            </ul>
        </div>
        <img src="{{flag}}" alt="country flag" class="flag-img">
    </div>
</div>
{{/each}}